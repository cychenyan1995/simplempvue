<view class="i-class i-checkbox" catchtap="checkboxChange">
    <!-- <i-cell i-class="i-checkbox-cell"> -->
        <label>
            <checkbox value="{{value}}" checked="{{checked}}" color="{{checked?color:''}}" disabled="{{disabled}}" class="i-checkbox-radio {{positionCls}}" />
            <view hidden="{{!label}}" class="i-checkbox-title">{{label}}</view>
        </label>
    <!-- </i-cell> -->
</view>
